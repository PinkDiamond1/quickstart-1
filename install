# digitalbits-core
wget -O digitalbits-core.deb https://dl.cloudsmith.io/public/xdb-foundation/digitalbits-core/deb/ubuntu/pool/focal/main/d/di/digitalbits-core_${DIGITALBITS_CORE_VERSION}_amd64.deb 
dpkg -i digitalbits-core.deb
rm digitalbits-core.deb

# frontier
wget -O frontier.deb https://dl.cloudsmith.io/public/xdb-foundation/digitalbits-frontier/deb/ubuntu/pool/focal/main/d/di/digitalbits-frontier_${FRONTIER_VERSION}_amd64.deb
dpkg -i frontier.deb
rm frontier.deb

echo "\nDone installing digitalbits-core and frontier...\n"
